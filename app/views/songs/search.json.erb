<% songs = @songs.map do |s|
  {
    id: s.id,
    artist: s.artist,
    title: s.title,
    canRequestAt: s.can_request_at.iso8601,
    canRequestIn: Time.now.utc - s.can_request_at
  }
end

res = {
  status: :ok,
  query: @query,
  time: Time.now.utc,
  results: songs,
  page: @page,
  has_more: @has_more
} %>

<%= res.to_json.html_safe %>
