<p id="notice"><%= notice %></p>

<h1>Listing Songs</h1>

<table>
  <thead>
    <tr>
      <th>Artist</th>
      <th>Title</th>
      <th>File size</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <%= render song %>
    <% end %>
  </tbody>
</table>

<%= paginate @songs %>

<br>

<%= link_to 'New Song', new_song_path %>
